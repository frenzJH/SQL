/** query celebrities by occupation**/
SELECT * FROM celebs WHERE occupation = 'Actor';
SELECT * FROM celebs WHERE occupation = 'Artist';
SELECT * FROM celebs WHERE occupation = 'Author';

/** Find actors and the movies they were in by joining celebs table to celeb_movie table by celeb.id and joining movies table to celeb_movie by movie.id **/

SELECT a.first_name, a.last_name, b.title as Movie FROM celeb_movie
JOIN celebs a
ON celeb_movie.celeb_id = a.id
JOIN movies b
ON celeb_movie.movie_id = b.id;

/** Find artists and their songs by joining celebs table to celeb_song table by celeb.id and joining songs table to celeb_song by song.id **/

SELECT a.first_name, a.last_name, b.title as Song FROM celeb_song
JOIN celebs a
ON celeb_song.celeb_id = a.id
JOIN songs b
ON celeb_song.song_id = b.id;

/** Find authors and the books they wrote by joining celebs table to celeb_book table by celeb.id and joining books table to celeb_book by book.id **/

SELECT a.first_name, a.last_name, b.title as Book FROM celeb_book
JOIN celebs a
ON celeb_book.celeb_id = a.id
JOIN books b
ON celeb_book.book_id = b.id;
